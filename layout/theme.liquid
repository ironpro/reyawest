          var hT = $('.product-single__form').offset().top,
              hH = $('.product-single__form').outerHeight(),
              wH = $(window).height(),
              wS = $(this).scrollTop();
          if (wS > (hT+hH-wH))
          {
            $('.sticky_add_cart').show();
          }
          if(wS < hT )
          {
            $('.sticky_add_cart').hide();
          }
        });




</ul>
<div class="clear"></div>
</div>
</div>
<style>
 
  .iconssection .page-width
  {
   max-width: 1060px;   margin-left: auto;
    margin-right: auto;padding: 0;
   
  }
.iconssection ul
  {
      padding: 0px;
    text-align: initial;
    width: 100%;
    float: left;
    margin: 0;
    text-align:center;
  }
  .iconssection ul li{
   display:inline-block;    
    vertical-align: top;
    width:19.6%;
  float:none;
    text-align:center;    margin: 0;
  }
  
 
   .iconssection ul li p
  {
    max-width:116px;
    margin: 0 auto;
/*     font-size:17px; */
    line-height:18px;
    text-align:center;
    margin-top:10px;
    font-weight:bold;
  }
  #iconssection{{ block.id }}  ul li p
  {
    color:{{ block.settings.headingcolor }}
  }
    .iconssection
  {
    padding-bottom:100px;padding-top: 25px;
  }
    .iconssection ul li span
  {
    display: block;
    max-width: 116px;
    text-align: center;
    margin: 0 auto;    height: 100px;
    vertical-align: middle;    position: relative;
  }
    .iconssection ul li span svg
  {
        max-width: 100%;
    height: auto;
    max-height: 100%;
  }
  .iconssection ul li span img, .iconssection ul li span svg
  {
    vertical-align: middle;
    max-height: 100%;
    line-height: 100px;    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
  }
  #iconssection{{ block.id }} .image-wrap svg
  {
    opacity:1;
  }
  #iconssection{{ block.id }} .image-wrap
  {
    height: 100%;
    position: relative;
  }
  #iconssection{{ block.id }}
  {
    padding:{{ block.settings.padding }};
    margin:{{ block.settings.margin }};
    {{ block.settings.secgradiant }}
  }
  @media(max-width:767px)
    {
       
      
  .iconssection ul li span 
   {
     height: 55px;
    }
       .iconssection ul li p
      {
      font-size: 15px;
    max-width: 100%;
      }
      .iconssection ul li span
      {
      max-width: 68px;
      }
      .iconssection
      {
    padding-bottom: 50px;
   padding-left: 10px;
    padding-right: 10px;    padding-top: 5px;
     }
       .iconssection ul li
      {
        padding-left: 8px;
    padding-right: 8px;
        width:24%
      }
    
      .iconssection ul li:nth-child({{ block.settings.hideonmob }})
      {
        display:none;
      }
     
      
      
       #iconssection{{ block.id }}
  {
    padding:{{ block.settings.padding_mob }};
    margin:{{ block.settings.margin_mob }};
    {{ block.settings.secgradiant_mob }}
  }
      
    }
  
</style>
          <div class="clear"></div>
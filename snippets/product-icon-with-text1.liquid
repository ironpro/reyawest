   {% else %}
   <div class="image-wrap">
              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
    {% endif %}
  </span>
<p>{{ block.settings.title2 }}</p>
   <div class="clearfix"></div>
</li>
   {% endif %}
 {% if   block.settings.title3 != blank %}

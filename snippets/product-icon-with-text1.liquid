<div class="clear"></div>
<div id="iconssection{{ block.id }}" class="iconssection" {{ block.shopify_attributes }}>
<div class="page-width">
<ul>
  {% if block.settings.title != blank %}
<li><span> 
   {% if block.settings.iconcode != blank %}
    {{ block.settings.iconcode }}
    {% elsif block.settings.icon %}
      <img src="{{  block.settings.icon | img_url:'large' }}">
  {% else %}
   <div class="image-wrap">
              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
    {% endif %}
  
  
  </span>
<p>{{ block.settings.title }}</p>
  <div class="clearfix"></div>
</li>
   {% endif %}
  {% if   block.settings.title2 != blank %}
<li><span>
   {% if block.settings.iconcode2 != blank %}

    color:{{  block.settings.quote1txt }};
    line-height: 24px;
    margin-left:40px;
  }
  
  .quote1 svg path
  {
    fill:{{ block.settings.quote1bg }}
  }
  
  .quote2 svg path
  {
    fill:{{ block.settings.quotebg2 }}
  }
  .quote3 svg path
  {
    fill:{{ block.settings.quotebg3 }}
  }
   .quote1 p

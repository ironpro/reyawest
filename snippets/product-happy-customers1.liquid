    display:block;
    margin:0 auto;
  }
 .cust_title  h2
  {
    font-size:45px;
    line-height:52px;
    color:{{ block.settings.titlecolor }};
    max-width:100%;
      margin: 0;
    letter-spacing: 0;
    text-align: center;
    {% if block.settings.bg_enable %}
    position:absolute;
    top:50%;
    left:50%;
    width:100%;
    transform:translate(-50%,-50%);
   
    {% endif %}
    
  
  }
  .quote1
  {
  
    border-radius:15px;
  
    max-width: 337px;
    padding: 20px 20px 0px 20px;

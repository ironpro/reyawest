<div class="clear"></div>
<!-- REVIEWS-->
<div id="customers{{ block.id }}" class="customers" {{ block.shopify_attributes }}>
<div class="page-width">
<div class="cust_title">
  
  {% if block.settings.bg_enable %}
  <div class="deskbnr">  {{ block.settings.titlebg_svg  }}</div>
  {% if block.settings.titlebg %}
  
  <img class="deskbnr" src="{{  block.settings.titlebg | img_url:'original' }}"> 
{% endif %}
    <div class="mobbnr">   {{ block.settings.titlemobbg_svg  }} </div>
  {% if block.settings.titlebgmob %}

  <img class="mobbnr" src="{{ block.settings.titlebgmob | img_url:'original' }}">
{% endif %}
  {% endif %}
  
  <h2>{{ block.settings.title }}</h2>
</div>
  
<div class="custwrap">
  
<div class="custimages">
  
   {% if block.settings.image %}
  <img src="{{ block.settings.image | img_url:'original' }}">
  {% endif %}
  
  </div>
  
<div class="quotes">
<div class="quote1">
  {{ block.settings.quote1SVG }}
 

  <div>
  
  <p>{{ block.settings.quote_text1 }}</p>
<p><b>{{ block.settings.quote_author1 }}</b></p>
  </div>
  
</div>
<div class="quote2">
{{ block.settings.quote2SVG }}
 
  
<div>
 <p>{{ block.settings.quote_text2 }}</p>
<p><b>{{ block.settings.quote_author2 }}</b></p>
</div>
</div>
<div class="quote3">
  {{ block.settings.quote3SVG }}

  
<div>
 <p>{{ block.settings.quote_text3 }}</p>
<p><b>{{ block.settings.quote_author3 }}</b></p>
</div>
</div>
</div>
  
<div class="clear"></div>
</div>
  
</div>
</div>
